{
  "name": "@blueeast/react-native-web-maps",
  "version": "0.1.0",
  "description": "React for Web implementation of react-native-maps",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/BlueEastCode/react-native-web-maps"
  },
  "author": {
    "name": "BlueEast Code",
    "url": "https://github.com/BlueEastCode/react-native-web-maps"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "keywords": [
    "react-native",
    "react-native-web",
    "react-native-web-maps"
  ],
  "scripts": {
    "build": "babel src --out-dir dist",
    "precommit": "lint-staged",
    "lint": "tslint -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls && codecov",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write --use-tabs --single-quote",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "react-native"
    ]
  },
  "release": {
    "branch": "master",
    "dryRun": false,
    "debug": true
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-native": "^4.0.0",
    "prettier": "^1.7.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-native-web": "^0.1.1",
    "tslint": "^5.4.3",
    "webpack": "^3.6.0",
    "semantic-release": "^14.0.0",
    "typescript": "^3.2.2"
  },
  "peerDependencies": {
    "react-native-web": "*"
  },
  "dependencies": {
    "react-google-maps": "^7.3.0",
    "tslint": "^5.11.0"
  }
}
